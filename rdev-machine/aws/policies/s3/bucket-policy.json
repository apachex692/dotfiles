{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowReadOnlyAccessForRole1",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::test-bucket-06122024-001",
        "arn:aws:s3:::test-bucket-06122024-001/*"
      ],
      "Principal": {
        "AWS": "arn:aws:iam::853973692277:role/test-role-06122024-001/*"
      }
    },
    {
      "Sid": "AllowFullAccessForRole2",
      "Effect": "Allow",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::test-bucket-06122024-001",
        "arn:aws:s3:::test-bucket-06122024-001/*"
      ],
      "Principal": {
        "AWS": "arn:aws:iam::853973692277:role/test-role-06122024-002/*"
      }
    },
    {
      "Sid": "DenyAllOtherAccess",
      "Effect": "Deny",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::test-bucket-06122024-001",
        "arn:aws:s3:::test-bucket-06122024-001/*"
      ],
      "Principal": "*"
    }
  ]
}
